  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6B6560",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#2ECC71"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#DA7756"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#DA7756"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#DA7756"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#E89B7B"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#9C9C9C"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#E89B7B"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#60A5FA"
      }
    },
    {
      "name": "Function Parameter",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#E5E0D8",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#FBBF24"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#FBBF24"
      }
    },
    {
      "name": "Interface",
      "scope": ["entity.name.type.interface"],
      "settings": {
        "foreground": "#22D3EE"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#E5E0D8"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "support.variable.property"],
      "settings": {
        "foreground": "#B0AAA0"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#E89B7B"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#DA7756"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#9C9C9C"
      }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#E89B7B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": {
        "foreground": "#60A5FA"
      }
    },
    {
      "name": "Markup Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#DA7756"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#E89B7B"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#22D3EE"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#E74C3C"
      }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#6B6560",
        "fontStyle": "strikethrough"
      }
    }
  ]
}
