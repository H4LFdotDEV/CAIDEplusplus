  "tokenColors": [
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#9C9C9C",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#3D9970"
      }
    },
    {
      "name": "Number",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#DA7756"
      }
    },
    {
      "name": "Boolean",
      "scope": ["constant.language.boolean"],
      "settings": {
        "foreground": "#DA7756"
      }
    },
    {
      "name": "Constant",
      "scope": ["constant", "constant.language", "constant.character"],
      "settings": {
        "foreground": "#DA7756"
      }
    },
    {
      "name": "Keyword",
      "scope": ["keyword", "keyword.control", "keyword.operator.new"],
      "settings": {
        "foreground": "#C96442"
      }
    },
    {
      "name": "Operator",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#6B6B6B"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#C96442"
      }
    },
    {
      "name": "Function",
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#4A90D9"
      }
    },
    {
      "name": "Function Parameter",
      "scope": ["variable.parameter", "meta.parameter"],
      "settings": {
        "foreground": "#1A1A1A",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Class",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#DAA520"
      }
    },
    {
      "name": "Type",
      "scope": ["entity.name.type", "support.type"],
      "settings": {
        "foreground": "#DAA520"
      }
    },
    {
      "name": "Interface",
      "scope": ["entity.name.type.interface"],
      "settings": {
        "foreground": "#17A2B8"
      }
    },
    {
      "name": "Variable",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#1A1A1A"
      }
    },
    {
      "name": "Property",
      "scope": ["variable.other.property", "support.variable.property"],
      "settings": {
        "foreground": "#4A4A4A"
      }
    },
    {
      "name": "Tag",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#C96442"
      }
    },
    {
      "name": "Attribute",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#DA7756"
      }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation"],
      "settings": {
        "foreground": "#6B6B6B"
      }
    },
    {
      "name": "Markup Heading",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#C96442",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Bold",
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markup Italic",
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markup Link",
      "scope": ["markup.underline.link", "string.other.link"],
      "settings": {
        "foreground": "#4A90D9"
      }
    },
    {
      "name": "Markup Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#DA7756"
      }
    },
    {
      "name": "JSON Key",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#C96442"
      }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#17A2B8"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#C4432B"
      }
    },
    {
      "name": "Deprecated",
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#9C9C9C",
        "fontStyle": "strikethrough"
      }
    }
  ]
}
